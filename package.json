    "lint-staged": "^9.0.0",
    "mockjs": "^1.0.1-beta3",
    "node-fetch": "^2.6.0",
    "postcss": "^7.0.27",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "pro-download": "1.0.1",
    "react-is": "^16.13.1",
    "style-loader": "^1.2.0",
    "stylelint": "^12.0.0",
    "umi-plugin-ga": "^1.1.3",
    "umi-plugin-pro": "^1.0.2",
    "umi-types": "^0.5.0",
    "webpack-dev-middleware": "^3.7.2"
  },
  "optionalDependencies": {},
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
