    "@types/lodash.debounce": "^4.0.6",
    "ace-builds": "^1.4.12",
    "ahooks": "^2.4.0",
    "animate.css": "^3.7.2",
    "antd": "4.13.1",
    "bizcharts": "^3.5.6",
    "bizcharts-plugin-slider": "^2.1.1",
    "classnames": "^2.2.6",
    "codemirror": "^5.54.0",
    "d3-force": "^3.0.0",
    "dva": "^2.6.0-beta.20",
    "echarts": "^5.0.2",
