    "nanoid": "^3.1.22",
    "numeral": "^2.0.6",
    "nzh": "^1.0.4",
    "omit.js": "^1.0.2",
    "path-to-regexp": "2.4.0",
    "prop-types": "^15.7.2",
    "qs": "^6.9.0",
    "query-string": "^6.10.1",
    "react": "^16.12.0",
    "react-ace": "^9.2.1",
    "react-codemirror2": "^7.1.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dnd": "11.1.3",
    "react-dnd-html5-backend": "11.1.3",
    "react-dom": "^16.12.0",
