    "@storybook/react": "^5.3.18",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.144",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.13",
    "@umijs/fabric": "2.0.8",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "chalk": "^3.0.0",
    "check-prettier": "^1.0.3",
    "cross-env": "^6.0.3",
    "cross-port-killer": "^1.1.1",
    "css-loader": "^3.5.3",
    "ejs": "^3.1.6",
    "enzyme": "^3.9.0",
    "express": "^4.17.1",
    "gh-pages": "^2.0.1",
    "http-proxy-middleware": "^1.0.3",
